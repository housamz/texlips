{
  "manifest_version": 2,
  "name": "Texlips",
  "version": "1.0",
  "permissions": ["activeTab", "contextMenus", "storage", "scripting"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
